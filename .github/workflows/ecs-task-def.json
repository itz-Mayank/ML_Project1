{
  "family": "studentperformance-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::591214163569:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "studentperformance",
      "image": "591214163569.dkr.ecr.us-east-1.amazonaws.com/studentperformance:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ]
    }
  ]
}
