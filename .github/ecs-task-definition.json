{
  "family": "studentperformance-task",
  "executionRoleArn": "arn:aws:iam::591214163569:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "studentperformance",
      "image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/studentperformance:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
